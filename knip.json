{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/main.ts",
    "src/preload.ts",
    "src/preload/notification.ts",
    "src/App.tsx",
    "src/renderer/notification/index.tsx"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": [
    "src/**/*.stories.tsx",
    "src/**/*.test.{ts,tsx}",
    "src/**/*.spec.{ts,tsx}",
    "**/__tests__/**",
    "**/__mocks__/**",
    "vite.*.config.ts"
  ],
  "ignoreDependencies": ["@electron-forge/*", "@electron/*", "@storybook/*"],
  "vitest": false,
  "vite": false,
  "eslint": {
    "config": ["eslint.config.js"]
  },
  "jest": {
    "config": ["jest.config.ts", "jest.database.config.ts"]
  },
  "playwright": {
    "config": ["playwright.config.ts"]
  },
  "typescript": {
    "config": ["tsconfig.json"]
  }
}
